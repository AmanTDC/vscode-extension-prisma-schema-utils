{
  "name": "prisma-schema-mapper",
  "displayName": "Prisma Schema Utils",
  "description": "A system to create mappings between snake case and camel case",
  "version": "0.0.4",
  "author": "Aman Gupta",
  "publisher": "Aman Gupta",
  "repository": "https://github.com/AmanTDC/vscode-extension-prisma-schema-utils",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:prisma-schema-mapper.snakeToCamel"
  ],
  "main": "./dist/extension.js",
  "icon": "icon.png",
  "contributes": {
    "commands": [
      {
				"command": "prisma-schema-mapper.snakeToCamel",
				"title": "Prisma Schema Convert",
				"when": "resourceLangId == prisma"
			}
    ],
    "menus": {
			"editor/context": [
				{
					"command": "prisma-schema-mapper.snakeToCamel",
					"when": "resourceLangId == prisma",
					"group": "1_modification"
				}
			],
			"explorer/context": [
				{
					"command": "prisma-schema-mapper.snakeToCamel",
					"when": "resourceLangId == prisma",
					"group": "1_modification"
				}
			]
		}
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.86.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "ts-loader": "^9.5.1",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9"
  }
}
